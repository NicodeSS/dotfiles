".zprezto":
  type: "git-repo"
  url: "https://github.com/sorin-ionescu/prezto"
  refreshPeriod: "168h"
  clone:
    args: ["--recursive"]

".zprezto/modules/fzf":
  type: "git-repo"
  url: "https://github.com/lildude/fzf-prezto.git"
  refreshPeriod: "168h"

".local/bin/client/git-open":
  type: "file"
  url: "https://raw.githubusercontent.com/paulirish/git-open/master/git-open"
  executable: true
  refreshPeriod: "168h"

".tmux/plugins/tpm":
  type: "git-repo"
  url: "https://github.com/tmux-plugins/tpm"
  refreshPeriod: "168h"

".zprezto/contrib":
  type: "git-repo"
  url: "https://github.com/belak/prezto-contrib"
  refreshPeriod: "168h"
  clone:
    args: ["--recurse-submodules=alias-tips,thefuck"]
  
    

